<h2>@($"{ViewData["AuthorName"]}\'s profile")</h2>
<a asp-controller="Verified" asp-action="Nodes">Back</a>
<table>
	<tr> <td>Id</td> <td>CreateTime</td> <td>Title</td> <td>Description</td> </tr>
	@foreach (Node node in Model)
	{
		<tr>
			<td>@node.Id</td>
			<td>@node.CreateDate</td>
			<td>@node.Title</td>
			<td>@node.Description</td>
		</tr>
	}
</table>